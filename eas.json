{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "http://localhost:8787/api/"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://ai-dnd.kopertop.workers.dev"
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://ai-dnd.kopertop.workers.dev"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleTeamId": "T2H2YPQXA9",
        "ascAppId": null
      }
    }
  }
}
