{
  "name": "ai-dnd-expo",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": ["react-native-fs"]
      }
    },
    "ios": {
      "bundleIdentifier": "org.coredumped.ai-dnd"
    }
  },
  "main": "expo-router/entry",
  "version": "1.1.5",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "train": "./ai-training/train.sh",
    "train:validate": "./ai-training/validate.sh",
    "train:convert": "python3.11 ai-training/simple_gguf_convert.py",
    "train:deploy": "python3.11 ai-training/deploy_to_cactus.py",
    "train:test": "python3.11 ai-training/test_deployed_model.py",
    "clean": "rm -rf .expo  dist/ ios/ android/ build*.ipa",
    "deploy": "run-s build:web build:web:* && wrangler deploy",
    "deploy:web": "run-s build:web build:web:* && wrangler deploy",
    "start": "expo start",
    "start:full": "./scripts/start-dev.sh",
    "start:worker": "cd workers && bun run dev",
    "dev": "wrangler dev",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "bun run format && bun run lint && bun run typecheck && bunx expo-doctor",
    "fix-imports": "bunx tsx scripts/fix-relative-imports.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:services": "vitest run --config vitest.services.config.ts",
    "test:services:watch": "vitest --config vitest.services.config.ts",
    "test:services:coverage": "vitest run --coverage --config vitest.services.config.ts",
    "test:api": "vitest run --config vitest.api.config.ts",
    "test:api:watch": "vitest --config vitest.api.config.ts",
    "test:all": "bun run test && bun run test:services && bun run test:api",
    "test:all:coverage": "bun run test:coverage && bun run test:services:coverage && bun run test:api",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "build:ios": "EXPO_NO_DOTENV=1 eas build --platform ios --profile production --local",
    "build:development": "eas build --profile development",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "build:web": "expo export --platform web -c -s",
    "build:web:assets": "cp -r assets/* dist/assets/",
    "db:migrate:dev": "wrangler d1 migrations apply DATABASE --local",
    "db:migrate:prod": "wrangler d1 migrations apply DATABASE --remote",
    "submit:staging": "./scripts/deploy.sh submit staging",
    "submit:production": "./scripts/deploy.sh submit production",
    "postversion": "perl -i -pe \"s/buildNumber: '(\\\\d+)'/sprintf(\\\"buildNumber: '%d'\\\", \\$1 + 1)/e\" app.ts",
    "postinstall": "node ./scripts/postinstall/apply-patches.js",
    "seed:maps": "bunx tsx scripts/seed-maps.ts",
    "seed:game": "bunx tsx scripts/seed-game-content.ts"
  },
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.7",
    "@expo/vector-icons": "^15.0.3",
    "@fugood/transformers": "^3.6.1-beta.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.8.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "2.2.12",
    "buffer": "^6.0.3",
    "expo": "^54.0.25",
    "expo-apple-authentication": "~8.0.7",
    "expo-asset": "~12.0.10",
    "expo-audio": "~1.0.14",
    "expo-auth-session": "^7.0.9",
    "expo-auth-template": "^1.0.10",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-speech-recognition": "^2.1.5",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-three": "^2.2.1",
    "expo-web-browser": "~15.0.9",
    "hono": "^4.10.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@cloudflare/vite-plugin": "^1.15.1",
    "@cloudflare/vitest-pool-workers": "^0.10.9",
    "@cloudflare/workers-types": "^4.20251120.0",
    "@eslint/compat": "^1.4.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/node": "^22.19.1",
    "@types/react": "~19.1.17",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^11.1.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "19.0.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^3.2.4",
    "vitest-react-native": "^0.1.5",
    "wrangler": "^4.49.1"
  },
  "eslintConfig": {
    "extends": [
      "expo"
    ],
    "rules": {
      "react/display-name": "off",
      "react-native/no-raw-text": "off"
    },
    "ignorePatterns": [
      "tests/**/*",
      "ai-training/llama.cpp/**/*"
    ]
  },
  "private": true
}
