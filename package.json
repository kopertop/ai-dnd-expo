{
  "name": "ai-dnd-expo",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "plugins": [
    "expo-apple-authentication"
  ],
  "main": "expo-router/entry",
  "version": "1.1.5",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --fix --quiet",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck && npx expo-doctor",
    "fix-imports": "npx tsx scripts/fix-relative-imports.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:services": "vitest run --config vitest.services.config.ts",
    "test:services:watch": "vitest --config vitest.services.config.ts",
    "test:services:coverage": "vitest run --coverage --config vitest.services.config.ts",
    "test:all": "npm run test && npm run test:services",
    "test:all:coverage": "npm run test:coverage && npm run test:services:coverage",
    "type-check": "tsc --noEmit",
    "build:ios": "EXPO_NO_DOTENV=1 eas build --platform ios --profile production --local",
    "build:development": "eas build --profile development",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "deploy:development": "./scripts/deploy.sh deploy development",
    "deploy:staging": "./scripts/deploy.sh deploy staging",
    "deploy:production": "./scripts/deploy.sh deploy production",
    "submit:staging": "./scripts/deploy.sh submit staging",
    "submit:production": "./scripts/deploy.sh submit production",
    "postversion": "perl -i -pe \"s/buildNumber: '(\\\\d+)'/sprintf(\\\"buildNumber: '%d'\\\", \\$1 + 1)/e\" app.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/ui": "~0.2.0-beta.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-ai/apple": "^0.5.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.0.14",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/react-query": "^5.90.2",
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.9",
    "@types/node-fetch": "^2.6.13",
    "ai": "^5.0.32",
    "buffer": "^6.0.3",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "expo": "~54.0.3",
    "expo-apple-authentication": "~8.0.7",
    "expo-audio": "~1.0.13",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.15",
    "expo-font": "~14.0.8",
    "expo-glass-effect": "~0.1.4",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.11",
    "expo-print": "~15.0.7",
    "expo-router": "^6.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-speech-recognition": "^2.1.1",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "~14.0.7",
    "expo-three": "^2.2.1",
    "expo-tracking-transparency": "~6.0.7",
    "expo-updates": "~29.0.11",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@cloudflare/vitest-pool-workers": "^0.8.56",
    "@cloudflare/workers-types": "^4.20250415.0",
    "@eslint/compat": "^1.2.8",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/dom-to-image": "^2.6.7",
    "@types/image-size": "^0.7.0",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.1",
    "react-test-renderer": "19.1.0",
    "tsx": "^4.19.3",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "wrangler": "^4.33.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
