{
  "name": "ai-dnd-expo",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "main": "expo-router/entry",
  "version": "1.1.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck && npx expo-doctor",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "build:development": "eas build --profile development",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "deploy:development": "./scripts/deploy.sh deploy development",
    "deploy:staging": "./scripts/deploy.sh deploy staging",
    "deploy:production": "./scripts/deploy.sh deploy production",
    "submit:staging": "./scripts/deploy.sh submit staging",
    "submit:production": "./scripts/deploy.sh submit production",
    "postversion": "node -e \"const fs = require('fs'); const pkg = require('./package.json'); const app = JSON.parse(fs.readFileSync('./app.json', 'utf8')); app.expo.runtimeVersion = pkg.version; fs.writeFileSync('./app.json', JSON.stringify(app, null, 2) + '\\n');\""
  },
  "dependencies": {
    "@eslint/compat": "^1.2.8",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "expo": "~53.0.17",
    "expo-audio": "~0.4.8",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-speech": "~13.1.7",
    "expo-speech-recognition": "^2.1.1",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-three": "^8.0.0",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "zod": "^3.25.76",
    "expo-dev-client": "~5.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": [
      "expo"
    ],
    "rules": {
      "react/display-name": "off",
      "react-native/no-raw-text": "off"
    }
  },
  "private": true
}
