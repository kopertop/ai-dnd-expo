{
  "name": "ai-dnd-expo",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": ["react-native-fs"]
      }
    },
    "ios": {
      "bundleIdentifier": "org.coredumped.ai-dnd"
    }
  },
  "main": "expo-router/entry",
  "version": "1.1.5",
  "packageManager": "bun@1.2.18",
  "scripts": {
    "train": "./ai-training/train.sh",
    "train:validate": "./ai-training/validate.sh",
    "train:convert": "python3.11 ai-training/simple_gguf_convert.py",
    "train:deploy": "python3.11 ai-training/deploy_to_cactus.py",
    "train:test": "python3.11 ai-training/test_deployed_model.py",
    "start": "expo start",
    "start:full": "./scripts/start-dev.sh",
    "start:worker": "cd workers && bun run dev",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "bun run format && bun run lint && bun run typecheck && bunx expo-doctor",
    "fix-imports": "bunx tsx scripts/fix-relative-imports.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:services": "vitest run --config vitest.services.config.ts",
    "test:services:watch": "vitest --config vitest.services.config.ts",
    "test:services:coverage": "vitest run --coverage --config vitest.services.config.ts",
    "test:all": "bun run test && bun run test:services",
    "test:all:coverage": "bun run test:coverage && bun run test:services:coverage",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "build:ios": "EXPO_NO_DOTENV=1 eas build --platform ios --profile production --local",
    "build:development": "eas build --profile development",
    "build:staging": "eas build --profile staging",
    "build:production": "eas build --profile production",
    "deploy:development": "./scripts/deploy.sh deploy development",
    "deploy:staging": "./scripts/deploy.sh deploy staging",
    "deploy:production": "./scripts/deploy.sh deploy production",
    "submit:staging": "./scripts/deploy.sh submit staging",
    "submit:production": "./scripts/deploy.sh submit production",
    "postversion": "perl -i -pe \"s/buildNumber: '(\\\\d+)'/sprintf(\\\"buildNumber: '%d'\\\", \\$1 + 1)/e\" app.ts",
    "postinstall": "node ./scripts/postinstall/apply-patches.js"
  },
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.7",
    "@expo/vector-icons": "^15.0.3",
    "@fugood/transformers": "^3.6.1-beta.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "2.2.12",
    "better-auth": "^1.3.34",
    "better-auth-cloudflare": "^0.2.8",
    "buffer": "^6.0.3",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.9",
    "expo-audio": "~1.0.14",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-speech": "~14.0.7",
    "expo-speech-recognition": "^2.1.1",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-three": "^2.2.1",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.9",
    "onnxruntime-common": "^1.22.0",
    "onnxruntime-react-native": "^1.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@better-auth/cli": "^1.3.34",
    "@cloudflare/workers-types": "^4.20241106.0",
    "@eslint/compat": "^1.2.8",
    "@playwright/test": "^1.49.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.3",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "react-test-renderer": "19.0.0",
    "tsx": "^4.20.3",
    "typescript": "~5.9.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vitest-react-native": "^0.1.5"
  },
  "eslintConfig": {
    "extends": [
      "expo"
    ],
    "rules": {
      "react/display-name": "off",
      "react-native/no-raw-text": "off"
    },
    "ignorePatterns": [
      "tests/**/*",
      "ai-training/llama.cpp/**/*"
    ]
  },
  "private": true
}
